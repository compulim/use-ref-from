{
  "extends": "./babel.production.config.json",
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "version": "7.22.6"
      }
    ]
  ],
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "ie": "11"
        }
      }
    ]
  ],
  "sourceMaps": true
}
