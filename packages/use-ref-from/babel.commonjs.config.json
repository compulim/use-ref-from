{
  "extends": "./babel.production.config.json",
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ],
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "modules": "commonjs",
        "targets": { "ie": "11" }
      }
    ]
  ],
  "sourceMaps": true
}
