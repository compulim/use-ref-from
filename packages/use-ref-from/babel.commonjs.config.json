{
  "extends": "./babel.production.config.json",
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "version": "7.24.1"
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowDeclareFields": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "modules": "commonjs",
        "targets": {
          "ie": "11"
        }
      }
    ]
  ],
  "sourceMaps": true
}
